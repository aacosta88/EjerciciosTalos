package com.talos.invertedindex.util;

import java.util.HashSet;
import java.util.List;

public class ArrayFunction {
	
	public static List<Integer> deleteDuplictedInteger(ArrayList<Integer> integerList){
		HashSet<String> hs = new HashSet<String>();
		hs.addAll(integerList);
		integerList.clear();
		integerList.addAll(hs);
		return integerList;
	}

}
